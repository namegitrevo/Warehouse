<Window x:Class="Warehouse.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Warehouse"
        mc:Ignorable="d"
        Title="Warehouse" Height="450" Width="800" MinHeight="450" MinWidth="800" >
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Grid.Row="0" HorizontalAlignment="Right">
            <TextBlock Text="{Binding ElementName=FrmMain, Path=Content.Title}" Style="{StaticResource Title}" Margin="10"/>
            <Image Source="Resources/logo.png" Width="60" Height="60" Margin="10"/>
        </StackPanel>
        <Frame x:Name="FrmMain" Grid.Row="1"
               NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
